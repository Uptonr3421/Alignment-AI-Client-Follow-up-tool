{
  "version": 2,
  "name": "lgbtq-center-gift-page",
  "buildCommand": null,
  "outputDirectory": ".",
  "framework": null,
  "installCommand": null,
  "routes": [
    {
      "src": "/(.*\.(exe|pdf|zip|msi))",
      "dest": "/$1",
      "headers": {
        "Content-Type": "application/octet-stream",
        "Content-Disposition": "attachment"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/gift-page.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
